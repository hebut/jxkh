																id="marrystate" mold="select" width="105px">
															</listbox>
														</hbox>
													</row>
													<row spans="1,5">
														<label
															value="健康状态" style="letter-spacing:1px">
														</label>
														<hbox>
															<textbox
																id="healthstate" width="150px" maxlength="25">
															</textbox>
														</hbox>
													</row>
													<row>
														<label
															value="身份证号" style="letter-spacing:1px" />
														<hbox>
															<textbox
																id="kuIdentity" width="250px" />
															<label
																value="*" style="color:red;valign:center" />
														</hbox>
														<label
															value="身份证扫描件" />
														<hbox>
															<button
																label="上传" id="identyFileUp" />
															<button
																label="删除" id="identyFileDel" visible="false" />
														</hbox>

													</row>
													<row spans="1,5">
														<label
															value="籍贯" style="letter-spacing:25px">
														</label>
														<textbox
															width="380px" id="nativeplace" maxlength="50">
														</textbox>
													</row>
												</rows>
											</grid>
											<hbox>
												<vbox>
													<hbox
														width="100%">
														<space
															width="5px" />
														<image id="img"
															width="150px" height="170px" />
													</hbox>
													<hbox
														width="100%">
														<space
															width="20px" />
														<button
															label="上传" id="upload" />
														<button
															label="删除" id="delete" />
													</hbox>
												</vbox>
											</hbox>
										</row>
									</rows>
								</grid>
							</row>
							<row>
								<grid>
									<columns>
										<column align="center"
											width="15%" />
										<column align="left" />
										<column align="center" />
										<column align="center"
											width="15%" />
										<column align="left" />
										<column align="left" />
									</columns>
									<rows>
										<row spans="1,5">
											<label value="所属部门"
												style="letter-spacing:3px" />
											<label id="deptSelect" />
										</row>
										<row spans="1,5">
											<label value="职称"
												style="letter-spacing:25px">
											</label>
											<hbox>
												<hbox id="selectTitle"
