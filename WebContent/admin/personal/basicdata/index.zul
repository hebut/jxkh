<?variable-resolver class="org.zkoss.zkplus.spring.DelegatingVariableResolver"?>
<zk>
	<window title="维护个人信息" width="100%" border="normal"
		style="text-align:left" sclass="newin"
		use="org.iti.jxkh.personal.PersonBasicWindow">
		<style>
			.red { color: red; font-style: oblique; font-size:18px; }
		</style>
		<caption>
			<button label="打印个人信息" id="print" target="blank" href="">
			</button>
			<space />
		</caption>
		<tabbox sclass="new-tabbox">
			<tabs>
				<tab label="基本信息" selected="true"  />
				<tab label="教育经历" />
				<tab label="工作信息"  />
				<tab label="职称信息" /> 
				<tab label="联系方式" />
			</tabs>
			<tabpanels>
				<!-- 基本信息 -->
				<tabpanel>
					<grid width="99%" height="515px">
						<rows>
							<row>
								<grid>
									<columns>
										<column width="35%"
											align="center" />
										<column width="15%" />
										<column width="25%"
											align="center" />
										<column width="25%" />
									</columns>
									<rows>
										<row spans="3,1">
											<grid width="100%">
												<columns>
													<column width="17%"
														align="center" />
													<column width=""
