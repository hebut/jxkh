																value="*" style="color:red;valign:center" />
														</hbox>
														<label
															value="性别" style="letter-spacing:25px" />
														<hbox>
															<radiogroup
																id="kuSex">
																<radio
																	label="男" checked="true" />
																<radio
																	label="女" />
															</radiogroup>
														</hbox>
													</row>
													<row>
														<label
															value="民族" style="letter-spacing:25px">
														</label>
														<hbox>
															<listbox
																id="kuNation" mold="select" width="155px">
															</listbox>
														</hbox>
														<label
