<?page title="new page title" contentType="text/html;charset=UTF-8"?>
<zk>
	<window title="承担国际交流合作项目（未开展）" border="normal" closable="true"
		use="com.uniwin.asm.personal.ui.data.teacherinfo.YqCdgjhzWindow"
		width="800px">
		<grid>
			<columns>
				<column width="17%" align="center" />
				<column width="33%" />
				<column width="15%" align="center" />
				<column width="35%" />
			</columns>
			<rows>
				<row >
					<label value="国际交流合作项目名称" />
					<hbox>
						<textbox id="cgmc" cols="30" />
						<label value="*"
							style="color:red;valign:center" />
					</hbox>
					<label value="合作对象" style="letter-spacing:10px" />
					<hbox>
						<textbox id="duixiang" cols="30" />
						<label value="*"
							style="color:red;valign:center" />
					</hbox>
				</row>
				<row>
					<label value="项目开始时间" />
					<hbox>
						<textbox id="Kssj" cols="30" />
						<label value="*"
							style="color:red;valign:center" />
					</hbox>
					<label value="项目结束时间" />
					<hbox>
						<textbox id="Jssj" cols="30" />
						<label value="*"
							style="color:red;valign:center" />
