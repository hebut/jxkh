<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
   <class name="org.iti.jxgl.entity.JxYear" table="JX_YEAR" schema="dbo" >
        <id name="id" type="java.lang.Long">
            <column name="ID" precision="18" scale="0" />
            <generator class="native" />
        </id>
        <property name="years" type="java.lang.String">
            <column name="YEARS" length="10" />
        </property>  
          <property name="yearsname" type="java.lang.String">
            <column name="YEARSNAME" length="10" />
        </property>    
        <property name="grade" type="java.lang.String">
            <column name="GRADE" length="10" />
        </property>  
    </class>
</hibernate-mapping>